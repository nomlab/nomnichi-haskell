User
    ident Text
    password Text Maybe
    salt Text
    UniqueUser ident
    deriving Typeable

 -- By default this file is used in Model.hs (which is imported by Foundation.hs)

Article
    user UserId
    title Text
    permaLink Text
    content Html
    createdOn UTCTime
    updatedOn UTCTime
    publishedOn UTCTime
    approved Bool
    count Int
    promoteHeadline Bool
    deriving

Comment
    commenter Text
    body Textarea
    createdAt UTCTime
    updatedAt UTCTime
    articleId ArticleId
    deriving
